include "../include/sllist.spl";

procedure concat(a: Node, b: Node)
  returns (res: Node)
  requires lseg(a, null) &*& lseg(b, null)
  ensures lseg(res, null)
{
  if (a == null) {
    return b;
  } else {
    var curr := a;
    while (curr.next != null) 
    {
      curr := curr.next; 
    }
    curr.next := b;
    return a;
  }
}
